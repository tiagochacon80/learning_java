import java.util.Scanner;
import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
	
		int codigo = sc.nextInt();
		int qtd = sc.nextInt();
		
		double preco;
		if (codigo == 1) {
			preco = qtd * 4.00;
		}
		else if (codigo == 2) {
			preco = qtd * 4.50;
		}
		else if (codigo == 3) {
			preco = qtd * 5.00;
		}
		else if (codigo == 4) {
			preco = qtd * 2.00;
		}
		else {
			preco = qtd * 1.50;
		}
		
		System.out.println("Item = " + codigo + " Quantidade = " + " Valor total= " + preco);
		
		
		sc.close();
	}
}
